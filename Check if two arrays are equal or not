#include <iostream>
#include<bits/stdc++.h>

using namespace std;
bool check(vector<int>arr1,vector<int>arr2,int n)
{
    sort(arr1.begin(),arr1.end());
    sort(arr2.begin(),arr2.end());
    for(int i=0;i<n;i++)
    {

        if(arr1[i]!=arr2[i])
        {
            return false;
        }
    }
    return true;
}
int main()
{
    int n;
    cin>>n;
    vector<int>arr1(n);
    for(int i=0;i<n;i++)
    {
        cin>>arr1[i];
    }
    vector<int>arr2(n);
    for(int i=0;i<n;i++)
    {
        cin>>arr2[i];
    }
    cout<<check(arr1,arr2,n);

    return 0;
}

A[] = {1,2,5,4,0}
B[] = {2,4,5,0,1}
