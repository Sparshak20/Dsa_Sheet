#include<bits/stdc++.h>
using namespace std;
int search(int arr[],int n,int x)
{
    for(int i=0; i<n; i++)
    {
        if(arr[i]==x)
        {
            return i;
        }

    }
    return -1;
}
int main()
{
    int arr[]= {20,5,7,25};
    int n=sizeof(arr)/sizeof(arr[0]);
    int x;
    cin>>x;
    cout<<search(arr,n,x);
    return 0;
}
//n = 4
//arr[] = {1,2,3,4}
//x = 3
//Output: 2

